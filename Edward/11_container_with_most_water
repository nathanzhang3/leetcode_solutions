class Solution(object):
    def maxArea(self, height):
        """
        :type height: List[int]
        :rtype: int
        """
        volume = 0
        for i in range(len(height)):
            for j in range(i+1, len(height)):
                temp = min(height[i], height[j]) * (j - i)
                if temp >= volume:
                    volume = temp
        return volume
